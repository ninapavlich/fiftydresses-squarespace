<footer id="page-footer" >
    
  <div class="inner-container" >
    <div class="pull-left" itemscope itemtype="http://schema.org/LocalBusiness">
      <h3 itemprop="name">{website.location.addressTitle}</h3>
      <address itemprop="address" >
      {website.location.addressLine1}<br />
      {website.location.addressLine2} {website.location.addressCountry}
      </address>
    </div>
    <div class="pull-right">
      {@|apply mailchimp.block}
    </div>
  </div>
</footer>