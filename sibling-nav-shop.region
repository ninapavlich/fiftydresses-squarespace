{.section collection}{.equal? typeName "products"}{.section categories}
<div class="subnav">
    <ul class="contain">
      <li>Filter:</li>

      <li><a href="{fullUrl}" {.if categoryFilter}{.or} class="active all"{.end}>All</a></li>
      {.repeated section @}
        <li><a href="{fullUrl}?category={@|url-encode}" {.equal? categoryFilter @} class="active"{.end}>{@}</a></li>
      {.end}
    </ul>
</div>
{.end}{.end}{.end}